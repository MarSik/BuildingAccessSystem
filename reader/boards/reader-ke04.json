{
  "platform": "freescalekinetis",
  "build": {
    "cpu": "cortex-m0plus",
    "f_cpu": "16000000L",
    "mcu": "MKE04P24M48SF0"
  },
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": [
            "-f", "scripts/interface/cmsis-dap.cfg",
            "-c", "set CHIPNAME kl04",
            "-f", "scripts/target/klx.cfg"
          ]
        },
        "preload": true,
        "onboard": true
      }
    }
  },
  "frameworks": ["mbed"],
  "name": "OK7MS MFRC522 over RS485 V.1",
  "upload": {
    "maximum_ram_size": 1024,
    "maximum_size": 8192
  },
  "url": "http://www.marsik.org",
  "vendor": "Ing. Martin Sivak"
}

